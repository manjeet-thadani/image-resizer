{
  "name": "image-resizer",
  "version": "1.0.0",
  "description": "The 'Image Resizer' is a utility tool that takes a image URL as a input and resize it to different sizes",
  "keywords": [
    "NodeJS",
    "TypeScript",
    "express",
    "image-resizer",
    "cloudinary"
  ],
  "author": "Manjeet Thadani",
  "main": "dist/server.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "lint": "tslint --project tsconfig.json",
    "dev": "nodemon --config nodemon.json src/server.ts",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "express": "4.17.1"
  },
  "devDependencies": {
    "@types/express": "4.17.9",
    "nodemon": "^2.0.6",
    "prettier": "^1.18.2",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=12.19.0"
  }
}
